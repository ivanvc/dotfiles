#text/html; w3m -I %{charset} -T text/html; copiousoutput;
#text/html; lynx -assume_charset=%{charset} -display_charset=utf-8 -collapse_br_tags -dump %s; nametemplate=%s.html; copiousoutput
text/html;     elinks -dump ; copiousoutput;
text/html;     xdg-open %s &> /dev/null &; nametemplate=%s.html
application/*; xdg-open %s &> /dev/null &;
text/calendar; cat %s | khal printics -f "When:{tab}{tab}{start-long} â†’ {end-long}{repeat-symbol}{nl}Event:{tab}{tab}{title}{nl}Status:{tab}{tab}{bold}{status}{reset}{nl}Organizer:{tab}{organizer}{nl}Location:{tab}{location}{nl}{nl}{description}"; copiousoutput;

text/*;        LESSQUIET=1 lesspipe.sh '%s'; copiousoutput
application/*; LESSQUIET=1 lesspipe.sh '%s'; copiousoutput
